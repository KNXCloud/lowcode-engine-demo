    {
      "title": "页面",
      "componentName": "Page",
      "props": [
        {
          "name": "style",
          "propType": "object",
          "defaultValue": {
            "padding": 12
          }
        }
      ],
      "configure": {
        "supports": {
          "style": true
        },
        "component": {
          "isContainer": true,
          "disableBehaviors": "*"
        }
      }
    },
    {
      "title": "插槽",
      "componentName": "Slot",
      "configure": {
        "component": {
          "isContainer": true,
          "disableBehaviors": "*"
        }
