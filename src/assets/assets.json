        {
          "name": "footer",
          "propType": "node"
        },
        {
          "name": "header-extra",
          "propType": "node"
        }
      ],
      "configure": {
        "supports": {
          "style": true,
          "events": ["onClose"],
          "loop": false
        },
        "component": {
          "isContainer": true
        },
        "props": [
          {
            "type": "group",
            "title": "卡片样式",
            "extraProps": {
              "display": "block"
